from tkinter import *


window = Tk()

window.geometry("400x500")

food_list = Menubutton(window, text="Mcdonald's menu")

listbox =Listbox(window,
                 bg="#ffddaa")
listbox.pack()

listbox.insert(1,"Fries=$2.99")
listbox.insert(2,"McFlurry=$1.99")
listbox.insert(3,"Mcdouble=$2.00")
listbox.insert(4,"Mcnuggets=$1.39")

food_list.pack()
window.mainloop()
